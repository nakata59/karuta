<div class="container">
  <div class="row">
    <div class="col-md-10 offset-md-1 col-lg-8 offset-lg-2">
      <h1>新規シリーズ作成</h1>
      <%= form_with model: @title, local: true do |f| %>
        <div class="form-group">
          <%= f.label :name %>
          <%= f.text_field :name, class: 'form-control' %>
        </div>
        <%= f.submit '作成', class: 'btn btn-primary' %>
      <% end %>
    </div>
  </div>
</div>