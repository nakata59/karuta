<%= form_with(model: @room) do |form| %>
  <div>
    <%= form.label :name, style: "display: block" %>
    <%= form.text_field :name %>
  </div>
  <div>
    <%= form.label :title_id, style: "display: block" %>
    <%= form.number_field :title_id %>
  </div>
  <div>
    <%= form.label :kazu, style: "display: block" %>
    <%= form.number_field :kazu %>
  </div>
  <div>
    <%= form.submit %>
  </div>
<% end %>

